    closeApp() {
        if (this.platform.is('android')) {
            this.platform.backButton.subscribeWithPriority(0, () => {
                console.log('this.router.url', this.router.url);
                if (this.router.url === '/home' || this.router.url === '/login') {
                    navigator['app'].exitApp();
