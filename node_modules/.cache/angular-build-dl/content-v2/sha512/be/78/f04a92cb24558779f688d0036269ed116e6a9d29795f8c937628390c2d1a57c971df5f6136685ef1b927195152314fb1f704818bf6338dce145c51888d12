            /*this.rootPage = "HomePage";*/
            this.rootPage = '/login';
        }
        this.sideMenu();
        this.initializeApp();
    }
    initializeApp() {
        this.platform.ready().then(() => {
            this.statusBar.styleDefault();
            this.splashScreen.hide();
            if (this.rootPage) {
                this.router.navigateByUrl(this.rootPage);
            }
        });
    }
