    getUserID() {
        return this.UserID;
    }
    getUserNickname() {
        this.httpClient.get(this.path + "/user/read.php?UserID=" + this.UserID)
            .subscribe(data => {
            localStorage.setItem('Nickname', JSON.stringify(data['Nickname']));
        }, error => {
            console.log(error);
        });
    }
