        this.UserID = parseInt(localStorage.getItem('UserID'));
        /*this.UserID = 1;*/
    }
    handleError(operation = 'operation', result) {
        return (error) => {
            console.error(error); // log to console instead
            return Object(rxjs__WEBPACK_IMPORTED_MODULE_4__["of"])(result);
        };
